{
  "components": {
    "schemas": {
      "ChartData": {
        "properties": {
          "x": {
            "description": "날짜 및 시간",
            "title": "X",
            "type": "string"
          },
          "y": {
            "description": "[시가, 고가, 종가, 저가]",
            "items": {
              "type": "number"
            },
            "title": "Y",
            "type": "array"
          }
        },
        "required": ["x", "y"],
        "title": "ChartData",
        "type": "object"
      },
      "ChartMatchingRequest": {
        "example": {
          "endDate": "2024-03-01T13:00:00Z",
          "startDate": "2024-03-01T12:00:00Z",
          "symbol": "BTCUSDT",
          "timeframe": "5"
        },
        "properties": {
          "endDate": {
            "description": "종료 날짜 및 시간",
            "format": "date-time",
            "title": "Enddate",
            "type": "string"
          },
          "startDate": {
            "description": "시작 날짜 및 시간",
            "format": "date-time",
            "title": "Startdate",
            "type": "string"
          },
          "symbol": {
            "description": "종목 코드 (예: BTCUSDT)",
            "title": "Symbol",
            "type": "string"
          },
          "timeframe": {
            "description": "시간 단위 (예: 5m, 1h, 1d)",
            "enum": [
              "1",
              "3",
              "5",
              "15",
              "30",
              "60",
              "120",
              "180",
              "240",
              "D",
              "W"
            ],
            "title": "Timeframe",
            "type": "string"
          }
        },
        "required": ["symbol", "timeframe", "startDate", "endDate"],
        "title": "ChartMatchingRequest",
        "type": "object"
      },
      "ChartMatchingResponse": {
        "properties": {
          "data": {
            "description": "차트 데이터",
            "items": {
              "$ref": "#/components/schemas/ChartData"
            },
            "title": "Data",
            "type": "array"
          },
          "similarity": {
            "description": "유사도 점수 (0~1)",
            "title": "Similarity",
            "type": "number"
          },
          "symbol": {
            "description": "종목 코드",
            "title": "Symbol",
            "type": "string"
          }
        },
        "required": ["symbol", "data", "similarity"],
        "title": "ChartMatchingResponse",
        "type": "object"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location",
            "type": "array"
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        },
        "required": ["loc", "msg", "type"],
        "title": "ValidationError",
        "type": "object"
      }
    }
  },
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/api/v1/chart_matching_list": {
      "post": {
        "description": "주어진 기간 동안의 차트 패턴과 유사한 패턴을 찾아 반환합니다.",
        "operationId": "chart_matching_list",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChartMatchingRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/ChartMatchingResponse"
                  },
                  "title": "Response Chart Matching List",
                  "type": "array"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "차트 패턴 매칭 리스트 조회",
        "tags": ["Chart Matching"]
      }
    },
    "/health": {
      "get": {
        "description": "API 서버의 상태를 확인하는 엔드포인트",
        "operationId": "health_health_get",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {}
              }
            },
            "description": "Successful Response"
          }
        },
        "summary": "Health",
        "tags": ["Health Check"]
      }
    }
  },
  "servers": [
    {
      "url": "/api"
    }
  ]
}
