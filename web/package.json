{
  "packageManager": "pnpm@9.14.2",
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "check": "biome check",
    "check:fix": "biome check --write --unsafe",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "prepare": "husky || true",
    "lint-staged": "lint-staged",
    "generate:api": "orval --config ./orval.config.ts && pnpm run check:fix"
  },
  "dependencies": {
    "@channel.io/channel-web-sdk-loader": "^2.0.0",
    "@orval/core": "^7.9.0",
    "@sentry/react": "^9.27.0",
    "@sentry/vite-plugin": "^3.5.0",
    "@tanstack/react-query": "^5.74.7",
    "@team-frieeren/components": "^1.0.9",
    "@team-frieeren/icons": "^1.0.0",
    "@vanilla-extract/css": "^1.17.1",
    "apexcharts": "^4.7.0",
    "axios": "^1.10.0",
    "i18next": "^25.1.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.539.0",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^19.0.0",
    "react-ga4": "^2.1.0",
    "react-i18next": "^15.5.1",
    "react-router": "^7.5.2",
    "react-ts-tradingview-widgets": "^1.2.8",
    "zod": "^3.24.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "^9.7.0",
    "@tanstack/react-query-devtools": "^5.74.7",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vanilla-extract/vite-plugin": "^5.0.1",
    "@vitejs/plugin-legacy": "^6.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "knip": "^5.52.0",
    "lint-staged": "^15.5.1",
    "msw": "^2.7.5",
    "orval": "^7.9.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "~5.7.2",
    "vite": "^6.3.1",
    "vite-plugin-compression2": "^1.3.3",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "biome check --write --unsafe"
    ]
  }
}
